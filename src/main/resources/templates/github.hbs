<!DOCTYPE html>  <!-- -*- mode: html -*- -->
<html>

<title>Test Github OAuth</title>
<body>

<div class="container">
    <h2>Session contents</h2>

    <ul>
        {{#session}}
            <li>{{key}} = {{value}} </li>
        {{/session}}
    </ul>

    <dl>
        <dt>userid</dt>
        <dd>{{userid}}</dd>
        <dt>name</dt>
        <dd>{{name}}</dd>
        <dt>avatar_url</dt>
        <dd>{{avatar_url}}</dd>
        <dt>email</dt>
        <dd>{{email}}</dd>
    </dl>
</div>

</body>
</html>